{
  "cacheDirectory": true,
  "babelrc": false,
  "presets": [
    ["@babel/preset-env", {
      "loose": true,
      "modules": false,
      "useBuiltIns": "usage",
      "sourceType": "unambiguous",
      "shippedProposals": true,
      "targets": {
        "browsers": [
          "last 2 versions",
          "IE >= 11"
        ]
      }
    }],
    ["@babel/preset-react", {
      "useBuiltIns": true,
      "pragma": "React.createElement"
    }]
  ],
  "plugins": [
    ["@babel/plugin-proposal-class-properties", {
      "loose": true
    }],
    "@babel/plugin-syntax-dynamic-import",
    ["@babel/plugin-transform-runtime", {
      "helpers": true,
      "regenerator": true,
      "polyfill": false
    }],
    "babel-plugin-emotion",
    "babel-plugin-add-module-exports"
  ]
}
